<h1><%= @project.name %></h1>
<p><%= link_to('Edit',  edit_project_path(@project)) %></p>
<p>Company: <%= @project.company.name %></p>
<p>Default Rate: <%= @project.default_rate %></p>
<p>Slug: <%= @project.slug %></p>

<p>All works</p>
<table>
  <tbody>
  <tr>
    <th>View</th>
    <th>User</th>
    <th>Project</th>
    <th>Date/Time</th>
    <th>Hours</th>
  </tr>
  <%= render partial: "works/work", collection: @project.works %>
  </tbody>
</table>


<% content_for :aside do %>
    <%= render 'sidebar' %>
<% end %>

